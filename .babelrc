{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "babel-plugin-css-modules-transform",
    ["@babel/plugin-proposal-private-property-in-object", { "loose": true }]
  ],
  "env": {
    "production": {
      "presets": [
        "minify"
      ]
    }
  }
}
